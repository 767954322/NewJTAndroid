<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">


    <TextView
        android:id="@+id/tv_fabu"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/font_28"
        android:layout_marginBottom="@dimen/font_15"
        android:layout_marginLeft="@dimen/font_12"
        android:background="@drawable/bg_edittext_normal_fabu"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/font_12"
        android:paddingRight="@dimen/font_12"
        android:textColor="@color/bg_262626"
        android:textSize="@dimen/size_12" />

    <TextView
        android:id="@+id/tv_fabu_select"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/font_28"
        android:layout_marginBottom="@dimen/font_15"
        android:layout_marginLeft="@dimen/font_12"
        android:background="@drawable/bg_edittext_select_fabu"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/font_12"
        android:paddingRight="@dimen/font_12"
        android:textColor="@color/white"
        android:textSize="@dimen/size_12"
        android:visibility="gone"/>

</RelativeLayout>




